<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>
<%@include file="/common/taglibs.jsp"%>
<!DOCTYPE>
<html>
<head>
<title>更新</title>
<link rel="stylesheet" href="${ctx}/css/organize/user.css" type="text/css">
</head>

<body>
<form autocomplete="off">
	<table class="new-user-table">
		<tr>
				<td>模板名称：</td>
				<td><input type="text" id="name" name="name" autofocus="autofocus" /></td>
			</tr>
			<tr>
				<td>视频码率：</td>
				<td><input type="text" id="rate" name="rate" /></td>
			</tr>
			<tr>
				<td>视频编码格式：</td>
				<td><input type="text" id="type" name="type" /></td>
			</tr>
			<tr>
				<td>视频帧率：</td>
				<td><input type="text" id="vframe" name="vframe" /></td>
			</tr>
			<tr>
				<td>封装格式：</td>
				<td><input type="text" id="fmt" name="fmt" /></td>
			</tr>
			<tr>
				<td>音频格式：</td>
				<td><input type="text" id="aframe" name="aframe" /></td>
			</tr>
			<tr>
				<td>分辨率高：</td>
				<td><input type="text" id="height" name="height" /></td>
			</tr>
			<tr>
				<td>分辨率宽：</td>
				<td><input type="text" id="width" name="width" /></td>
			</tr>
			<tr>
				<td>画面比例：</td>
				<td><input type="text" id="proportion" name="proportion" /></td>
			</tr>
			<tr>
				<td>所属商：</td>
				<td><input type="text" id="companyid" name="companyid" /></td>
			</tr>
			<tr>
				<td>是否启用：</td>
				<td><input type="radio" name="isused" value="0" />启用
				<input type="radio" name="isused" value="1" />停用
				</td>
			</tr>
			<tr>
				<td>模板描述：</td>
				<td>
				<textarea id="desc" name="desc"  style="width: 267px;"></textarea>
				</td>
			</tr>
		<tr>
			<td colspan="2"><a href="javascript:updateMaterialTemplate();" class="save-btn"> 保 存 </a>&nbsp;<a href="javascript:layer.closeAll();" class="cancel-btn"> 取 消 </a></td>
		</tr>
	</table>
	</form>
	<input type="hidden" value="" id="_id" name="_id">
</body>
</html>
