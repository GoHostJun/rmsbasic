<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>
<%@include file="/common/taglibs.jsp"%>
<!DOCTYPE>
<html>
<head>
<title>修改微信消息模板</title>
<link rel="stylesheet" href="${ctx}/css/organize/user.css" type="text/css">
</head>

<body>
	<table class="new-user-table">
		<tr>
			<td>消息模板名称：</td>
			<td><input type="text" id="title" name="title" autofocus="autofocus" /><span id="wechat_title">*</span></td></td>
		</tr>
		<tr>
			<td>消息模板类型：</td>
			<td><input type="text" id="type" name="type"  disabled="disabled"/></td></td>
		</tr>
		<tr>
			<td>消息模板id：</td>
			<td><input type="text" id="template_id" name="template_id" /><span id="wechat_template_id">*</span></td>
		</tr>
		<tr>
			<td colspan="2"><a href="javascript:updateWechattemp();" class="save-btn"> 保 存 </a>&nbsp;<a href="javascript:layer.closeAll();" class="cancel-btn"> 取 消 </a></td>
		</tr>
	</table>
</body>
</html>
