<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>
<%@include file="/common/taglibs.jsp"%>
<!DOCTYPE>
<html>
<head>
<title>添加扫描</title>
<link rel="stylesheet" href="${ctx}/css/organize/user.css" type="text/css">
</head>

<body>
	<form id="form" name="form">
		<table class="new-user-table">
			<tr>
				<td>扫描源目录：</td>
				<td><input type="text" id="srcdic" name="srcdic" autofocus="autofocus" /></td></td>
			</tr>
			<tr>
				<td>扫描文件类型：</td>
				<td><input type="text" id="dictype" name="dictype"  /></td></td>
			</tr>
			<tr>
				<td>扫描人id：</td>
				<td><input type="text" id="taruserid" name="taruserid" /></td>
			</tr>
			<tr>
				<td>扫描客户标识：</td>
				<td><input type="text" id="companyid" name="companyid" /></td>
			</tr>
			<tr>
				<td>是否启用：</td>
				<td>
				<input type="radio" name="isused"  value="0"/>停用
				<input type="radio"name="isused" value="1"/ checked>启用
				</td>
			</tr>
			<tr>
				<td>扫描类型：</td>
				<td><input type="text" id="scantype" name="scantype" /></td>
			</tr>
			<tr>
				<td>扫描服务标识：</td>
				<td><input type="text" id="tardic" name="tardic" /></td>
			</tr>
			<tr>
				<td>扫描服务应用标识：</td>
				<td><input type="text" id="appcode" name="appcode" /></td>
			</tr>
			<tr>
				<td>扫描完成注册地址：</td>
				<td><input type="text" id="url" name="url" /></td>
			</tr>
			<tr>
				<td>扫描服务：</td>
				<td><input type="text" id="service" name="service" /></td>
			</tr>
			<tr>
				<td colspan="2"><a href="javascript:addScan();" class="save-btn"> 保 存 </a>&nbsp;<a href="javascript:layer.closeAll();" class="cancel-btn"> 取 消 </a></td>
			</tr>
		</table>
	</form>
</body>
</html>
