<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>
<%@include file="/common/taglibs.jsp"%>
<link rel="stylesheet" href="${ctx}/css/organize/user.css" type="text/css">
<script src="${ctx }/js/pushpath/pushpath_update.js" type="text/javascript" charset="utf-8"></script>
<script src="${ctx }/js/pushpath/pushpath_view.js" type="text/javascript" charset="utf-8"></script>
	<table class="new-user-table">
	<input type="hidden" id="_id" name="_id">
		<tr>
			<td>名称：</td>
			<td><input type="text" id="pushName" name="pushName"  /><span id="push_name">*</span></td>
		</tr>
		<tr>
			<td>唯一标识：</td>
			<td><input type="text" id="uniqueName" name="uniqueName"  /><span id="unique_name">*</span></td>
		</tr>
		<tr>
			<td>推送地址：</td>
			<td><input type="text" id="pushUrl" name="pushUrl" /><span id="push_url">*</span></td>
		</tr>
		<tr>
			<td>配置信息：</td>
			<td><input type="text" id="queryUrl" name="queryUrl" /></td>
		</tr>
		<tr>
			<td>其它信息：</td>
			<td><textarea id="otherMsg" style="width: 270px;    padding-left: 10px;"></textarea><td>
		</tr>
		<tr>
			<td colspan="2"><a href="javascript:updatePushPath();" class="save-btn"> 保 存 </a>&nbsp;<a href="javascript:layer.closeAll();" class="cancel-btn"> 取 消 </a></td>
		</tr>
	</table>
