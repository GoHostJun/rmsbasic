.down_list>li.down_list4 ul{padding: 0 0 0 62%;}



/*footer*/
.footer { width:100%; background:#3c3c3c; min-width:960px; margin-top:100px; padding:24px 0;}
.footer p { font-size:12px; color:#7d7d7d; text-align:center; line-height:2em;}
.footerJXIndex {
	position: absolute;
	left: 0;
	bottom: 0;
	z-index: 9;
	width: 100%;
	height: 60px;
	line-height: 24px;
	background: #FFF;
	border-top:1px solid #edeff0;
	padding:16px 0 0;
}
.footerJXIndex p {
	color: #999;
	font-size: 12px;
}
.footerJX{
	background: #FFF;
    border-top: 1px solid #ccc;
}
/*页面路径*/
p.p_url { width:88%; margin:0 auto; min-width:960px; max-width:1200px; clear:both; font-size:14px; color:#ffad00; height:62px; line-height:62px;}
p.p_url i { width:13px; height:12px; background:url(../images/header_icon.png) no-repeat -44px -1px; float:left; margin:26px 4px 0 0;}
p.p_url a { color:#ffad00;}
/*按钮*/
.btn_div .btn { display:inline-block; margin-right:20px; height:42px; line-height:42px; width:138px; text-align:center; font-size:16px; border-radius:5px;}
.btn_div .btn.btn_green { color:#13ce00; border:1px solid #13ce00;}
