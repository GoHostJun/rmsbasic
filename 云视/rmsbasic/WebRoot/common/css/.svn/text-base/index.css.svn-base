.check_cont .check_div h3 b.green { color:#13ce00;}
.check_cont .video_div { position: relative; width:100%; background:#000; border:2px solid #333;}
.check_cont .video_div p { font-size:16px; color:#FFF; height:30px; line-height:30px;}
.check_cont .video_div i { position:absolute; top:35%; left:41%; z-index:10; width:70px; height:70px; background:url(../images/big_video_icon.png) no-repeat 0 0;cursor: pointer;}


.check_cont .sh_div { margin-top:30px; clear:both;}
.check_cont .sh_div p { font-size:16px; color:#999; clear:both; padding:16px 24px 0; line-height:2em;}
