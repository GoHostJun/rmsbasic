@charset "utf-8";
/* CSS Document */

/*公共样式*/
* { margin:0; padding:0; font-family:Microsoft YaHei;}
img { display:block; border:none;}
ul,li,dl,dt,dd { list-style:none;}
h1,h2,h3,h4,h5,h6,div,ul,ol,li,dl,dt,dd,iframe,textarea,input,button,p,strong,b,a,span,del,pre,table,tr,th,td { margin:0; padding:0;}
.fl { float:left;}
.fr { float:right;}
.clear { clear:both; margin:0; padding:0; font-size:0;}
i { display:inline-block;}
a { text-decoration:none;}
input,textarea{outline:none; box-sizing:content-box;}
.stand{display: inline-block;vertical-align: top;}
.hide { display:none;}
.show{display:block;}




.inner { width:88%; margin:0 auto; min-width:960px; max-width:1200px;}
/*#c00405*/
.header { width:100%; height:50px; background:#d93024; min-width:960px;}
.header .user_div i.help_icon { float:left; width:26px; height:26px; margin:12px 20px 0 0; background:url(../images/header_icon.png) no-repeat -1px -1px; cursor:pointer;}
.header .user_div img { float:left; width:36px; height:36px; margin:7px 20px 0 0;}
.header .user_div #user_menu { font-size:16px; color:#f4f4f4; margin:14px 14px 0 0; cursor:pointer;}
.header .user_div #user_menu .user_menu_i{ width:13px; height:8px; background:url(../images/header_icon.png) no-repeat -3px -38px; margin-left:12px;}
/*lsj*/
.user_div .user_pop{position: absolute;display: none;top:30px;font-style:normal;top:24px;left:-88px;}
.user_div .user_pop .user_pop_triangle{width: 10px;height: 10px;background: #fff;position: absolute;top: -4px;left: 90px; z-index: 1000;
  -ms-transform: rotate(45deg);
  -moz-transform: rotate(45deg);
  -webkit-transform: rotate(45deg);
  -o-transform: rotate(45deg);
}
/*lsj2010-12-19*/
.msg_btn{display: inline-block;position: relative; height: 24px;width: 30px;background: url(../images/msg_btn.png)no-repeat;background-size:100% 100%;vertical-align: middle;margin-top: 16px; cursor:pointer;}
.msg_btn .num{display: block;top: -10px;left: 20px; width: 22px;height: 22px;position: absolute; color: #fff;text-align: center;line-height: 22px;background: #c11508;border-radius: 100%;font-size: 12px;}
.msg_btn_down{display: none; position: absolute;right: 0px;top: 40px; /*height: 234px;*/width: 234px;padding-top: 15px; border: solid 1px #ffad00;background: #fff;z-index: 666;border-radius: 8px;}
.msg_btn_down i{display: block; width: 30px;height: 16px; padding-top:5px;background: #fff;position: absolute;left:202px;top:-18px; background: url(../images/pop_bg.png) no-repeat 0 6px;}
.msg_btn_down ul{padding: 0 5px;/*height:230px;overflow:auto;*/}
.msg_btn_down li{border-bottom: dotted 1px #ccc;height: 34px;line-height: 34px;}
.msg_btn_down li span{font-size: 14px !important;color: #666 !important;}
.msg_btn_down li span + span{float: right;}
.msg_btn_down li .msg_li_cont{display: inline-block; width: 140px;white-space: nowrap; overflow: hidden;text-overflow: ellipsis;margin-right: 5px;}
.msg_btn_down li a{color: #d02020;} /*text-decoration:underline;*/


.user_pop { position:absolute;}
.user_pop_inner{position: absolute;width: 110px;height: 84px;border: solid 1px #999;z-index: 999;background: #fff;font-size: 14px;color: #666;padding-top: 10px;}
.user_pop_inner li{height: 26px;line-height: 26px;}
.user_pop_inner li:hover {background: #ccc;cursor: pointer;}
.user_pop_inner li b{display: inline-block; height: 16px;width: 16px;vertical-align: top;margin: 5px 10px 0 10px}
.user_pop_inner li:nth-child(2) b{background: url(../images/icon5.png) no-repeat 0px 0px;}
.user_pop_inner li:nth-child(3) b{background: url(../images/icon5.png) no-repeat -19px 0px;}
.user_pop_inner li:nth-child(1) b{background: url(../images/icon5.png) no-repeat -38px 0px;}
/*弹窗*/
.popup{ display:none;position: fixed;width: 100%;height: 100%;top: 0px;left: 0px;background: rgba(111,111,111,0.2);z-index: 1100;}
.popup_inner{width: 500px;height: 300px;border: solid 1px #ffad00;margin: 0 auto;margin-top: 138px;background: #fff;}
.popup_inner .top{height: 40px;background: #ffad00;line-height: 40px;padding-left: 10px;color: #fff; font-size: 14px;}
.popup_inner .top i{cursor: pointer; width: 16px;height: 16px;background: url(../images/close_btn.png)no-repeat;display: inline-block;float: right;margin: 12px 9px 0 0;}
.popup_cont{padding-top: 24px;}
.user_message_popup .popup_cont .head_img{width: 139px;height: 160px;/* border: solid 1px #ffad00; */margin: 0 0 0 24px;}
.user_message_popup .popup_cont .head_img>div{border: dashed 1px #ffad00;width: 120px;height: 140px;margin: 10px 0 0 9px;background: url(../images/head_img.png)no-repeat;background-size: 120px 140px;}
.user_message_popup .popup_cont .user_message{width: 250px;height: 160px;font-size: 14px;color: #333;}
.user_message_popup .popup_cont .user_message li{height: 30px;line-height: 30px;padding-left: 20px;}
.user_message_popup .popup_cont .circle_cont{height: 160px;width: 150px;}
.popup_cont .edit_btn_div a{border-radius:3px;height: 36px;width: 130px;font-size: 16px;line-height: 36px;cursor: pointer;}
.popup_cont .edit_btn_div .save_btn{margin-left: 100px;}
.user_message_popup .popup_cont .user_message li input{display: none;height: 26px;width: 150px;padding-right:10px ;border: none;display: inline-block;font-size: 14px;color: #333;}
.user_message_popup .popup_cont .user_message li input:focus{background:#ccc;}



.nav { position:fixed;left:0; top:0;z-index:99; width:100%; height:102px; min-width:960px;}
.nav .inner { height:102px;}
.nav .logo { position:relative;  margin-left:10px; margin-top: 23px;}
.nav .nav_line { width:100%;}
.nav .nav_div a{height: 88px;}
.nav .nav_div a { display:block; float:left; width:64px; padding:0 9px; margin-right:10px; text-align:center; color:#fff; font-size:16px; margin-top:18px;}
.nav .nav_div a i { display:block; margin:0 auto 6px; width:37px; height:41px;}
.nav .nav_div a i.i1 { background:url(../images/nav_icon.png) no-repeat 0 0;}
.nav .nav_div a i.i2 { background:url(../images/nav_icon.png) no-repeat 0 -54px;}
.nav .nav_div a i.i3 { background:url(../images/nav_icon.png) no-repeat 0 -103px;}
.nav .nav_div a i.i4 { background:url(../images/nav_icon.png) no-repeat 0 -155px;}
.nav .nav_div a i.i5 { background:url(../images/nav_icon.png) no-repeat 0 -209px;}
