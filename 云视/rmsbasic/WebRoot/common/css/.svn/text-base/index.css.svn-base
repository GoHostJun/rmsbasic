.check_cont .sh_div p i.sh_icon1 { background:url(../images/header_icon.png) no-repeat 0 -65px;}
.check_cont .sh_div p i.sh_icon2 { background:url(../images/header_icon.png) no-repeat -21px -65px;}
.check_cont .sh_div p i.sh_icon3 { background:url(../images/header_icon.png) no-repeat -42px -65px;}
.check_cont .sh_div p b { float:right; font-weight:normal; font-size:12px; color:#999;}
/*lsj*/
.check_cont .sh_div .sh_thanks{display: none; position: absolute;top: 24px;left: -10px; z-index:99;font-size: 14px;color: #666;text-align: left; line-height: 30px; width: 182px;border: solid 1px #ffad00;border-radius: 8px;font-style: normal;padding:0 20px;background: #fff;}
.check_cont .sh_div .sh_thanks i{display: block; width: 16px;height: 16px;background: #fff;position: absolute; background: url(../images/pop_bg.png)no-repeat; top: -20px;left: 0;}
.video_audio_div { clear:both; padding-top:30px;}
.video_audio_div h3 { margin-bottom:16px;}
.check_cont .check_div h3 a i.add_img { background:url(../images/header_icon.png) no-repeat -29px -47px;}
.check_cont .check_div h3 a i.del_img { background:url(../images/header_icon.png) no-repeat -45px -47px;}
.video_audio_div .img_cont { width:44%; margin:14px 0 0 0; float:left;}
