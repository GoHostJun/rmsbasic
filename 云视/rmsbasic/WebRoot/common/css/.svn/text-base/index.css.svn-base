/*lsj*/
.nav .user_div{margin-top: 2px;}
.nav .nav_div a.active {color: #fff;}
.nav .nav_div a.active i.i1 { background:url(../images/nav_icon_a.png) no-repeat 0 0;}
.nav .nav_div a.active i.i2 { background:url(../images/nav_icon_a.png) no-repeat 0 -54px;}
.nav .nav_div a.active i.i3 { background:url(../images/nav_icon_a.png) no-repeat 0 -103px;}
.nav .nav_div a.active i.i4 { background:url(../images/nav_icon_a.png) no-repeat 0 -155px;}
.nav .nav_div a.active i.i5 { background:url(../images/nav_icon_a.png) no-repeat 0 -209px;}
.nav .nav_div a.active i.i6 { background:url(../images/nav_icon_gg.png) no-repeat -51px 0;}
.nav .nav_div a:hover i.i1 { background:url(../images/nav_icon_a.png) no-repeat 0 0;}
.nav .nav_div a:hover i.i2 { background:url(../images/nav_icon_a.png) no-repeat 0 -54px;}
.nav .nav_div a:hover i.i3 { background:url(../images/nav_icon_a.png) no-repeat 0 -103px;}
.nav .nav_div a:hover i.i4 { background:url(../images/nav_icon_a.png) no-repeat 0 -155px;}
.nav .nav_div a:hover i.i5 { background:url(../images/nav_icon_a.png) no-repeat 0 -209px;}
.nav .nav_div a:hover i.i6 { background:url(../images/nav_icon_gg.png) no-repeat -51px 0;}
.nav .nav_div a:hover {color: #fff !important;}
/*导航下拉*/
/*.down_list li{display: none; }*/
.down_list>li{display: none;height: 60px;line-height: 60px;width: 100%;background: #fff;z-index: 950;position: absolute;border-bottom: solid 1px #d7d7d7;}
.down_list>li ul{padding: 0 0 0 46%;}
.down_list>li:nth-child(2) ul{padding: 0 0 0 51%;}
