/*首页*/
/*内容头部*/
.content_top{height: 182px;width: 100%;}
.content_top ul{font-size: 0px; clear:both;}
.content_top li{display: inline-block;vertical-align: top;width: 23.25%;height: 120px;margin-right: 2.33%;margin-top: 30px;overflow:hidden;}
.content_top li:last-child{margin-right: 0px;}
.content_top li:nth-child(1){background: #27a9e3;}
.content_top li:nth-child(2){background: #28b779;}
.content_top li:nth-child(3){background: #852b99;}
.content_top li:nth-child(4){background: #ffb848;}
.content_top li .li_cont{height: 90px;width: 100%;}
.content_top li .li_cont .li_cont_bg{display: inline-block;width: 64px;height: 62px;margin-left: 10px;margin-top: 15px;}
.content_top li:nth-child(1) .li_cont_bg{background:url(../images/icon2.png)no-repeat -192px 0px;}
.content_top li:nth-child(2) .li_cont_bg{background:url(../images/icon2.png)no-repeat 0px 0px;}
.content_top li:nth-child(3) .li_cont_bg{background:url(../images/icon2.png)no-repeat -94px 0px;}
.content_top li:nth-child(4) .li_cont_bg{background:url(../images/icon2.png)no-repeat -274px 0px;}
.content_top li .li_cont .li_num{display: inline-block;width: 130px;color: #fff;font-size: 36px;margin-top: 22px;margin-right: 10px;text-align: right;font-family: arial;}
.content_top li .li_num p{font-size: 14px;}
.content_top  .li_bottom{height: 30px;width: 100%;font-size: 12px;color:rgba(250,250,250,0.8);line-height: 30px;background: rgba(105,105,105,0.2);}
.content_top  .li_bottom p{margin-left: 10px;display: inline-block;}
.content_top  .li_bottom .li_bottom_bg{height: 15px;width: 15px;display: inline-block;background: url(../images/syncfusion-icons-white.png)no-repeat -27px -10px;margin: 8px 10px 0  0;cursor: pointer;}

.list-newstl .li_bottom p{width:65%;white-space:nowrap;overflow:hidden;text-overflow:ellipsis;}
.content_top li{position: relative;}
.content_top li img{position: absolute;}
.tl-up{width:100%;height:100%;background:#ffb848;position:absolute;top:100%;}
.content_top li:last-child:hover .tl-up{top:0; transition:top 1s; -webkit-transition:top 1s;}
.tl-up-inner li{background:none !important;cursor: pointer;width:52px;height:100%;margin-top:0px;}
.tl-up-inner{width:;margin:0 auto;height:100%;}
.tl-up-inner{text-align:center;}
.tl-up-inner li{display:inline-block;margin:0 10px;}
.tl-up i{display:block;width:52px;height:52px;margin-top:26px;}
.tl-up .tl-up-lf i{background:url(../images/icon21.png)no-repeat  -72px 0px;;}
.tl-up .tl-up-rg i{background:url(../images/icon21.png)no-repeat}
.tl-up p{font-size:14px;color:#fff;margin-top:8px; margin-left: -9px;}

