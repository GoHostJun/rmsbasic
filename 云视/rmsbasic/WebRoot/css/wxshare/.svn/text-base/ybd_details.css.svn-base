html,body{
	width: 100%;
	height: 100%;
	background: #f6f6f6;
}
.logo{
	width: 117px;
	height: 25px;
	margin-top: 8px;
	
}
.container{
	padding: 0 2%;
}
.top-fixed{
	height: 40px;
	background: #fff;
	line-height: 40px;
	font-size: 14px;
	position: fixed;
	top: 0;
	width: 96%;
	z-index: 999;
}
.top-fixed i{
	display: inline-block;
	width: 12px;
	height: 17px;
	background: url(../../images/wxshare/iphone.png)no-repeat 0px 0px;
	vertical-align: middle;
	background-size: 100%;
	margin-right: 5px;
	margin-top: -3px;
}
.top-fixed a{
	text-decoration: underline;
	color: #e03e3f;
}
.title-cont{
	padding-top: 40px;
	margin: 14px 0;
}
.title-cont h2{
	color: #333;
	font-size: 20px;
	font-weight: 700;
	margin-bottom: 12px;
	-white-space:nowrap;
    -overflow:hidden;
    -text-overflow:ellipsis;
}
.title-cont  p{
	color: #999;
	font-size: 14px;
	padding-bottom: 12px;
	border-bottom: dashed 1px #ccc;

}
.title-cont  p span{
	margin-left:10px;

}
.text-cont{
	margin-top: 10px;
	padding-bottom: 80px;
}
.text-cont p{
	color: #333;
	font-size: 16px;
	line-height: 1.5;
	text-align: justify;
	
}
.bottom-fixed{
	width: 100%;
	height: 50px;
	background: #fff;
	position: fixed;
	bottom: 0;
	box-shadow: 0 -5px 20px #ccc;
}
.btn-group {
	width: 280px;
	margin: 10px auto;
}
.btn-group a{
	display: block;
	float: left;
	height: 30px;
	width: 125px;
	border-radius: 30px;
	background: #E03E3F;
	text-align: center;
	color: #fff;
	line-height: 30px;
}
.btn-group a i{
	display: inline-block;
	vertical-align: middle;
	width: 20px;
	height: 20px;
	margin-right: 5px;
	margin-top: -4px;
}
.inport-btn{
	width: 180px !important;
	display: block;
	margin: 0 auto !important;
	float: none !important;
}
.inport-btn i{
	background: url(../../images/wxshare/icon1.png)no-repeat  -46px -46px;
	background-size: 400%;
	margin-top: -7px !important;
}
.ture-btn i{
	background: url(../../images/wxshare/icon1.png)no-repeat  0px -62px;
	background-size: 500%;
}
.cancle-btn{
	background: #cccccc !important;
	color: #666 !important;
	margin-left: 30px;
}
.cancle-btn i{
	background: url(../../images/wxshare/icon1.png)no-repeat  -29px -62px;
	background-size: 500%;
}
/*审核成功弹窗*/
.popup{
	display:none;
	top: 0;
	height: 100%;
	width: 100%;
	position: fixed;
	background: rgba(0,0,0,0.4);
	z-index: 9999;
}
.popup-inner{
	width: 150px;
	height: 45px;
	margin: 0 auto;
	position: relative;
	top: 50%;
	margin-top: -52.5px;
	background: #fff;
	border-radius: 5px;
	left: 0;
	right: 0;
	padding: 30px 30px;
	
}
.popup-inner h2{
	font-size: 20px;
	font-weight: 700;
}
.popup-inner p{
	font-size: 12px;
	color: #999;
	margin-top: 2px;
}
.circle-ic{
	height: 42px;
	width: 42px;
	border-radius: 50%;
	margin: 3px 4px 0 0 ;

}
.audit-success .circle-ic{
	background: #00cc00;
}
.audit-success .circle-ic i{
	background: url(../../images/wxshare/icon1.png)no-repeat -60px -3px;;
	background-size: 400%;
}
.audit-failed .circle-ic{
	background: #cc3333;
}
.audit-failed .circle-ic i{
	background: url(../../images/wxshare/icon1.png)no-repeat -36px -3px;;
	background-size: 400%;
}
.circle-ic i{
	display: block;
	width: 22px;
	height: 22px;
	margin: 10px auto;
	
}
/*轮播*/
.bannar{
	overflow: hidden;
}
.news_content {
	overflow-y: auto;
}
.device {
  position: relative;
}
.swiper-slide img {
  width: 100% ;
  height:100% ;
}
.pagination {
  position: absolute;
  left: 0;
  z-index: 99;
  text-align: center;
  bottom:5px;
  width: 100%;
}
.swiper-pagination-switch {
  display: inline-block;
  width: 10px;
  height: 10px;
  border-radius: 10px;
  background: #999;
  box-shadow: 0px 1px 2px #555 inset;
  margin: 0 3px;
  cursor: pointer;
}
.swiper-active-switch {
  background: #fff;
}
/**蒙版*/
.share_tip {
	position: fixed;
	left: 0;
	top: 0;
	z-index: 999;
	width: 100%;
	height: 100%;
	background: rgba(0,0,0,.8);
}
.share_tip img {
	width: 88%;
	margin:10px auto 0;
	max-width: 380px;
}
.share_tip a {
	display: block;
	width: 30%;
	margin:30px auto 0;
}
.share_tip a img {
	width: 100%;
	max-width: 150px;
}
.share_tip_ios {
	position: fixed;
	left: 0;
	top: 0;
	z-index: 999;
	width: 100%;
	height: 100%;
	background: rgba(0,0,0,.8);
}
.share_tip_ios img {
	width: 88%;
	margin:10px auto 0;
	max-width: 380px;
}
.share_tip_ios a {
	display: block;
	width: 30%;
	margin:30px auto 0;
}
.share_tip_ios a img {
	width: 100%;
	max-width: 150px;
}
.img-list {
	margin-top: 10px;
	width: 100%;
	overflow-x: scroll !important;
}
.img-list > div{
	height: 40px;
	width: 100%;
	background: #fff
	z-index: 999;
	overflow: hidden;
}
.img-list li{
	float: left;
	margin:0 5px;
	position: relative;
}
/*.img-list li:first-child{
	margin: 0;
}*/
.img-list li img{
	width: 100%;
	height: 100%;
}
.img-list li i{
	display: block;
	width: 30px;
	height: 30px;
	background: url(../../images/wxshare/play.png)no-repeat;
	background-size: cover;
	position: absolute;
	top: 50%;
	left: 0;
	right: 0;
	margin: 0 auto;
	margin-top: -27px;
	
}
.img-show{
	width: 100%;
	height: 100%;
}
.img-show img{
	width: 100%;
	height: 100%;
}
.video-show{
	width: 100%;
	height: 100%;
}
.video-show video,.video-show img{
	width: 100%;
	height: 100%;
	background: #000;
}
.img-list-div{
	overflow: hidden;
}
